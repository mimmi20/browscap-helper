{
  "name": "mimmi20/browscap-helper",
  "description": "some small helper functions for working with Browscap",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/browscap-helper/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-json": "*",
    "browscap/browscap": "dev-update-dependencies",
    "browscap/browscap-php": "^4.1",
    "donatj/phpuseragentparser": "dev-master",
    "endorphin-studio/browser-detector": "*",
    "jaybizzle/crawler-detect": "*",
    "localheinz/json-normalizer": "^0.9",
    "mimmi20/browscap-helper-source": "dev-update-dependencies",
    "mimmi20/browser-detector": "dev-updates",
    "mimmi20/browser-detector-tests": "dev-updates",
    "mimmi20/browser-detector-version": "~6.0.2",
    "mimmi20/json-class": "~2.0.1",
    "mimmi20/ua-result": "~6.0.0",
    "mobiledetect/mobiledetectlib": "dev-master",
    "piwik/device-detector": "dev-master",
    "psr/cache": "^1.0",
    "psr/log": "^1.0",
    "sinergi/browser-detector": "dev-master",
    "symfony/cache": "^4.2",
    "symfony/console": "^4.2",
    "symfony/finder": "^4.2",
    "symfony/yaml": "^4.2",
    "ua-parser/uap-core": "dev-master",
    "whichbrowser/parser": "dev-master",
    "woothee/woothee": "dev-master",
    "woothee/woothee-testset": "dev-master",
    "yzalis/ua-parser": "dev-master",
    "zsxsoft/php-useragent": "dev-master"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.13",
    "localheinz/composer-normalize": "^1.0.0",
    "localheinz/phpstan-rules": "^0.4",
    "pepakriz/phpstan-exception-rules": "^0.3",
    "phpstan/phpstan": "^0.10",
    "phpstan/phpstan-deprecation-rules": "^0.10",
    "phpstan/phpstan-strict-rules": "^0.10",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "BrowscapHelper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BrowscapHelperTest\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/mimmi20/uap-core"
    },
    {
      "type": "git",
      "url": "https://github.com/mimmi20/browscap"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "bin": [
    "bin/browscap-helper.php"
  ]
}
