{
  "name": "mimmi20/browscap-helper",
  "description": "some small helper functions for working with Browscap",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/browscap-helper/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.2.0 || ^8.0.0",
    "ext-json": "*",
    "ext-simplexml": "*",
    "browscap/browscap": "~6.0.40",
    "browscap/browscap-php": "~4.2.2 || ~4.3.0 || ~5.0.0",
    "donatj/phpuseragentparser": "~v1.1.0",
    "endorphin-studio/browser-detector": "~6.0.0",
    "ergebnis/json-normalizer": "~0.12.0 || ~0.13.0 || ~0.14.0",
    "jaybizzle/crawler-detect": "~v1.2.102",
    "matomo/device-detector": "dev-master",
    "mimmi20/browser-detector": "~5.0.77",
    "mimmi20/browser-detector-version": "~6.0.16",
    "mimmi20/json-class": "~2.0.15",
    "mimmi20/json-normalizer": "~1.0.8",
    "mimmi20/ua-result": "~7.1.6",
    "mobiledetect/mobiledetectlib": "~2.8.34",
    "psr/cache": "~1.0.1",
    "psr/log": "~1.1.3",
    "psr/simple-cache": "~1.0.1",
    "sinergi/browser-detector": "~6.1.3",
    "symfony/cache": "~4.4.11 || ~5.1.8",
    "symfony/console": "~4.4.11 || ~5.1.8",
    "symfony/finder": "~4.4.11 || ~5.1.8",
    "symfony/yaml": "~4.4.11 || ~5.1.8",
    "ua-parser/uap-core": "dev-master",
    "whichbrowser/parser": "~2.0.42",
    "woothee/woothee": "~v1.10.0",
    "woothee/woothee-testset": "~v1.11.1",
    "yzalis/ua-parser": "dev-master",
    "zsxsoft/php-useragent": "dev-master"
  },
  "require-dev": {
    "ext-pdo": "*",
    "mimmi20/coding-standard": "~1.0.6",
    "pepakriz/phpstan-exception-rules": "~0.11.1",
    "phpstan/extension-installer": "~1.0.5",
    "phpstan/phpstan": "~0.12.57",
    "phpstan/phpstan-deprecation-rules": "~0.12.5"
  },
  "config": {
    "platform": {
      "php": "7.2.0"
    },
    "preferred-install": {
      "browscap/browscap": "source",
      "donatj/phpuseragentparser": "source",
      "endorphin-studio/browser-detector": "source",
      "jaybizzle/crawler-detect": "source",
      "mimmi20/browser-detector": "source",
      "mobiledetect/mobiledetectlib": "source",
      "piwik/device-detector": "source",
      "sinergi/browser-detector": "source",
      "ua-parser/uap-core": "source",
      "whichbrowser/parser": "source",
      "woothee/woothee-testset": "source",
      "yzalis/ua-parser": "source",
      "zsxsoft/php-useragent": "source",
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "BrowscapHelper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BrowscapHelperTest\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/mimmi20/uap-core"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "bin": [
    "bin/browscap-helper.php"
  ]
}
