{
  "name": "mimmi20/browscap-helper",
  "description": "some small helper functions for working with Browscap",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/browscap-helper/graphs/contributors"
    }
  ],
  "require": {
    "php": "^8.1.0",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "browscap/browscap": "^6.1.x-dev",
    "cbschuld/browser.php": "^v1.9.6",
    "donatj/phpuseragentparser": "^v1.7.0",
    "endorphin-studio/browser-detector": "^6.0.1",
    "endorphin-studio/browser-detector-tests": "^2.2.0 || ^3.1.0",
    "ergebnis/json-normalizer": "^2.2.0",
    "jaybizzle/crawler-detect": "^v1.2.111",
    "matomo/device-detector": "dev-master",
    "mimmi20/browscap-helper-source": "^3.0.16",
    "mimmi20/browser-detector": "^7.0.2",
    "mimmi20/browser-detector-version": "^7.0.8",
    "mimmi20/ua-result": "^8.0.7",
    "mobiledetect/mobiledetectlib": "^2.8.39",
    "psr/cache": "^1.0.1 || ^2.0.0 || ^3.0.0",
    "psr/log": "^3.0.0",
    "psr/simple-cache": "^3.0.0",
    "sinergi/browser-detector": "^6.1.4",
    "symfony/console": "^v6.1.4",
    "symfony/finder": "^v6.1.3",
    "symfony/yaml": "^v6.1.4",
    "whichbrowser/parser": "^2.1.7",
    "woothee/woothee-testset": "^v1.13.0",
    "zsxsoft/php-useragent": "^1.5.0"
  },
  "require-dev": {
    "mimmi20/coding-standard": "^3.0.10",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "^1.8.3",
    "phpstan/phpstan-deprecation-rules": "^1.0.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "BrowscapHelper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BrowscapHelperTest\\": "tests/"
    }
  },
  "bin": [
    "bin/browscap-helper.php"
  ],
  "config": {
    "preferred-install": {
      "browscap/browscap": "source",
      "cbschuld/browser.php": "source",
      "donatj/phpuseragentparser": "source",
      "endorphin-studio/browser-detector-tests": "source",
      "mimmi20/browser-detector": "source",
      "mobiledetect/mobiledetectlib": "source",
      "matomo/device-detector": "source",
      "jaybizzle/crawler-detect": "source",
      "sinergi/browser-detector": "source",
      "whichbrowser/parser": "source",
      "woothee/woothee-testset": "source",
      "yzalis/ua-parser": "source",
      "zsxsoft/php-useragent": "source",
      "*": "dist"
    },
    "lock": false,
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true
    }
  }
}
