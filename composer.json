{
  "name": "mimmi20/browscap-helper",
  "description": "some small helper functions for working with Browscap",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/browscap-helper/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-json": "*",
    "browscap/browscap": "dev-update-dependencies",
    "browscap/browscap-php": "^4.2.0",
    "donatj/phpuseragentparser": "dev-master",
    "endorphin-studio/browser-detector": "*",
    "jaybizzle/crawler-detect": "*",
    "localheinz/json-normalizer": "^0.9.0",
    "mimmi20/browscap-helper-source": "~2.6.1",
    "mimmi20/browser-detector": "~5.0.36",
    "mimmi20/browser-detector-tests": "~2.0.83",
    "mimmi20/browser-detector-version": "~6.0.7",
    "mimmi20/json-class": "~2.0.5",
    "mimmi20/ua-result": "~7.0.3",
    "mobiledetect/mobiledetectlib": "dev-master",
    "piwik/device-detector": "dev-master",
    "psr/cache": "^1.0.0",
    "psr/log": "^1.1.0",
    "sinergi/browser-detector": "dev-master",
    "symfony/cache": "^4.3.0",
    "symfony/console": "^4.3.0",
    "symfony/finder": "^4.3.0",
    "symfony/yaml": "^4.3.0",
    "ua-parser/uap-core": "dev-master",
    "whichbrowser/parser": "dev-master",
    "woothee/woothee": "dev-master",
    "woothee/woothee-testset": "dev-master",
    "yzalis/ua-parser": "dev-master",
    "zsxsoft/php-useragent": "dev-master"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.15.0",
    "localheinz/composer-normalize": "^1.3.0",
    "localheinz/phpstan-rules": "^0.13.0",
    "pepakriz/phpstan-exception-rules": "^0.8.0",
    "phpstan/phpstan": "^0.11.17",
    "phpstan/phpstan-deprecation-rules": "^0.11.0",
    "phpstan/phpstan-strict-rules": "^0.11.0",
    "squizlabs/php_codesniffer": "^3.5.1"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "BrowscapHelper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BrowscapHelperTest\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/mimmi20/uap-core"
    },
    {
      "type": "git",
      "url": "https://github.com/mimmi20/browscap"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "bin": [
    "bin/browscap-helper.php"
  ]
}
