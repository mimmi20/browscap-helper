{
  "name": "mimmi20/browscap-helper",
  "description": "some small helper functions for working with Browscap",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/browscap-helper/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.4.3 || ^8.0.0",
    "ext-json": "*",
    "ext-simplexml": "*",
    "ergebnis/json-normalizer": "^1.0.3",
    "matomo/device-detector": "dev-master",
    "mimmi20/browser-detector": "^6.0.6",
    "mimmi20/browser-detector-version": "^7.0.7",
    "mimmi20/json-class": "^4.0.0",
    "mimmi20/json-normalizer": "^2.0.6",
    "mimmi20/ua-result": "^8.0.6",
    "psr/log": "^1.1.4 || ^3.0.0",
    "psr/simple-cache": "^1.0.1",
    "symfony/cache": "^5.4.0",
    "symfony/console": "^5.3.11",
    "symfony/finder": "^5.3.7",
    "symfony/yaml": "^5.4.0",
    "whichbrowser/parser": "^2.1.2",
    "woothee/woothee-testset": "^v1.13.0"
  },
  "require-dev": {
    "mimmi20/coding-standard": "^2.5.1",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "^1.2.0",
    "phpstan/phpstan-deprecation-rules": "^1.0.0",
    "rector/rector": "^0.12.5"
  },
  "config": {
    "preferred-install": {
      "browscap/browscap": "source",
      "mimmi20/browser-detector": "source",
      "piwik/device-detector": "source",
      "whichbrowser/parser": "source",
      "woothee/woothee-testset": "source",
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "BrowscapHelper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BrowscapHelperTest\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "bin": [
    "bin/browscap-helper.php"
  ]
}
