{
  "name": "mimmi20/browscap-helper",
  "description": "some small helper functions for working with Browscap",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/browscap-helper/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-json": "*",
    "ext-simplexml": "*",
    "browscap/browscap": "dev-master",
    "browscap/browscap-php": "^4.2.2",
    "donatj/phpuseragentparser": "dev-master",
    "endorphin-studio/browser-detector": "^5.0.0",
    "ergebnis/json-normalizer": "^0.10.1 || ^0.11.0",
    "jaybizzle/crawler-detect": "dev-master",
    "mimmi20/browser-detector": "~5.0.42",
    "mimmi20/browser-detector-version": "~6.0.9",
    "mimmi20/json-class": "~2.0.8",
    "mimmi20/ua-result": "~7.0.5",
    "mobiledetect/mobiledetectlib": "dev-master",
    "piwik/device-detector": "dev-master",
    "psr/cache": "^1.0.0",
    "psr/log": "^1.1.2",
    "psr/simple-cache": "^1.0.1",
    "sinergi/browser-detector": "dev-master",
    "symfony/cache": "^4.4.2 || ^5.0.2",
    "symfony/console": "^4.4.2 || ^5.0.2",
    "symfony/finder": "^4.4.2 || ^5.0.2",
    "symfony/yaml": "^4.4.2 || ^5.0.2",
    "ua-parser/uap-core": "dev-master",
    "whichbrowser/parser": "dev-master",
    "woothee/woothee": "dev-master",
    "woothee/woothee-testset": "dev-master",
    "yzalis/ua-parser": "dev-master",
    "zsxsoft/php-useragent": "dev-master"
  },
  "require-dev": {
    "ext-pdo": "*",
    "ergebnis/composer-normalize": "^2.2.2",
    "ergebnis/phpstan-rules": "^0.14.2",
    "friendsofphp/php-cs-fixer": "^2.16.1",
    "pepakriz/phpstan-exception-rules": "^0.10.1",
    "phpstan/phpstan": "^0.12.8",
    "phpstan/phpstan-deprecation-rules": "^0.12.2",
    "phpstan/phpstan-strict-rules": "^0.12.2",
    "squizlabs/php_codesniffer": "^3.5.3"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": {
      "browscap/browscap": "source",
      "donatj/phpuseragentparser": "source",
      "mobiledetect/mobiledetectlib": "source",
      "piwik/device-detector": "source",
      "sinergi/browser-detector": "source",
      "ua-parser/uap-core": "source",
      "whichbrowser/parser": "source",
      "woothee/woothee-testset": "source",
      "yzalis/ua-parser": "source",
      "zsxsoft/php-useragent": "source",
      "jaybizzle/crawler-detect": "source",
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "BrowscapHelper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BrowscapHelperTest\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/mimmi20/uap-core"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "bin": [
    "bin/browscap-helper.php"
  ]
}
