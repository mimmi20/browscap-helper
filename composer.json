{
  "name": "mimmi20/browscap-helper",
  "description": "some small helper functions for working with Browscap",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/browscap-helper/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.2.0 || ^8.0.0",
    "ext-json": "*",
    "ext-simplexml": "*",
    "browscap/browscap": "^6.0.42",
    "browscap/browscap-php": "^4.3.0 || ^5.0.0",
    "ergebnis/json-normalizer": "^0.12.0 || ^0.13.0 || ^0.14.0 || ^1.0.2",
    "matomo/device-detector": "dev-master",
    "mimmi20/browser-detector": "^5.0.82",
    "mimmi20/browser-detector-version": "^6.0.18",
    "mimmi20/json-class": "^2.0.17",
    "mimmi20/json-normalizer": "^1.0.11",
    "mimmi20/ua-result": "^7.1.8",
    "psr/cache": "^1.0.1 || ^3.0.0",
    "psr/log": "^1.1.3",
    "psr/simple-cache": "^1.0.1",
    "symfony/cache": "^4.4.19 || ^5.2.3",
    "symfony/console": "^4.4.19 || ^5.2.3",
    "symfony/finder": "^4.4.19 || ^5.2.3",
    "symfony/yaml": "^4.4.19 || ^5.2.3",
    "whichbrowser/parser": "^2.1.1",
    "woothee/woothee-testset": "^v1.11.1"
  },
  "require-dev": {
    "mimmi20/coding-standard": "^1.0.10",
    "pepakriz/phpstan-exception-rules": "^0.11.6",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "^0.12.78",
    "phpstan/phpstan-deprecation-rules": "^0.12.6"
  },
  "config": {
    "platform": {
      "php": "7.2.0"
    },
    "preferred-install": {
      "browscap/browscap": "source",
      "mimmi20/browser-detector": "source",
      "piwik/device-detector": "source",
      "whichbrowser/parser": "source",
      "woothee/woothee-testset": "source",
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "BrowscapHelper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BrowscapHelperTest\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "bin": [
    "bin/browscap-helper.php"
  ]
}
